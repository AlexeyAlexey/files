<h1>Listing file_folders</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @file_folders.each do |file_folder| %>
      <tr>
        <td><%= link_to 'Show', file_folder %></td>
        <td><%= link_to 'Edit', edit_file_folder_path(file_folder) %></td>
        <td><%= link_to 'Destroy', file_folder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File folder', new_file_folder_path %>
